{
  "name": "nq-tslib-base",
  "version": "0.0.1",
  "description": "在H5页面中调用快应用功能",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "srctype": "ts",
  "scripts": {
    "rename": "node rename.js",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js src/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.js src/**/*.ts --fix",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "test": "npm run lint && nyc --nycrc-path .nycrc-ts mocha",
    "coveralls": "nyc --nycrc-path .nycrc-ts report --reporter=text-lcov | coveralls",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git push && git tag $npm_package_version && git push --tags"
  },
  "scripts:js": {
    "rename": "node rename.js",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js src/**/*.js",
    "lint:fix": "eslint --fix -c .eslintrc.js src/**/*.js",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "mocha": "mocha --require @babel/register ./test/**/*.js",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "author": "nianqin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nianqin/nq-tslib-base.git"
  },
  "bugs": {
    "url": "https://github.com/nianqin/nq-tslib-base/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-istanbul": "^5.2.0",
    "cdkit": "^1.3.0",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "es5-shim": "^4.5.13",
    "eslint": "^6.1.0",
    "expect.js": "^0.3.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "rollup-plugin-uglify": "^6.0.2",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@jsmini/extend": "^0.3.1",
    "@jsmini/type": "^0.8.0"
  }
}
